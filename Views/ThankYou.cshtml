@{
    ViewData["Title"] = "Thank You";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"]</title>
</head>
<body>
    <div class="container">
        <h2>Thank You for Completing the Mental Health Assessment</h2>
        <p>Your score is: @ViewBag.Score / 9</p>

        <p>If you are feeling overwhelmed or need further assistance, please consider reaching out to a mental health professional.</p>
    </div>
</body>
</html>
